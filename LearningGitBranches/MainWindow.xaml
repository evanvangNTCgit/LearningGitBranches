<Window x:Class="LearningGitBranches.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LearningGitBranches"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid x:Name="pictureOfEvan">
            <Button x:Name="firstCommitButton" Grid.Column="0" Grid.Row="0" Content="First Commit Button" Click="firstCommitButton_Click"/>
            <Button x:Name="projectDateButton" Grid.Column="1" Grid.Row="0" Content="How long has this project been up?" Click="Button_Click"/>
            <Button Grid.Column="2" Grid.Row="0" Content="See picture of Evan" Click="Button_Click_1"/>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
        </Grid>
    </Grid>
</Window>
